{
	"name": "Debian",
	"image": "ghcr.io/prulloac/base-devcontainer:latest",
	"features": {
		"ghcr.io/prulloac/devcontainer-features/chezmoi:1": {}
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"GitHub.copilot",
				"GitHub.copilot-chat",
				"GitHub.copilot-labs",
				"foam.foam-vscode",
				"bierner.github-markdown-preview",
				"github.vscode-github-actions",
				"mhutchie.git-graph",
				"yzhang.markdown-all-in-one"
			]
		}
	},
	"updateContentCommand": "chezmoi init prulloac && chezmoi apply"
}
